
.CompareBody {
    height: 100%;
    margin: 0;
    background-color: rgb(222, 222, 222);
}
#CompareWap {
    width: 100%;
    height: 100%;
}
.chart-content {
   background-color:white;
}
.eleChart {
    width: 49%;
    height: 50%;
    display: inline-block;
    border: 1px solid #C7C7C7;
}

.chart-content > div:nth-child(1) {
    
    border-right: 0;
}

.chart-content > div:nth-child(3) {
    border-top: 0;
}

.chart-content > div:nth-child(4) {
    border-left: 0;
    border-top: 0;
}

#CompareWap .textbox {
    border-radius: unset;
    border: 1px solid #b3bbb3;
}

#Time_Part .textbox {
    width: 98px !important;
    height: 28px !important;
}
.linksearch {
    padding: 8px 25px;
    background-color: #40c08d;
    color: #fff;
    text-decoration: none;
    font-weight: bold;
    font-size: 15px;
    padding: 2px 15px 4px;
    top: -1px;
}
