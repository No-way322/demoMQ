.btn-list {
	width: 60%;
	margin: 0 auto;
	/* margin-left: 80px; */
}


.register-table {
	width: 80%;
	height: 60%;
	margin: 10px auto;
}

.register-table .tb-left {
	text-align: right;
	width: 35%;
}

.hide {
	display: none !important;
}

.rcu-statistics {
	height: 40px;
}

.rcu-statistics .rcu-statistics-item {
	display: inline-block;
	margin: 5px;
}

.rcu-statistics .rcu-statistics-item .item-label {
}

.rcu-statistics .rcu-statistics-item .item-text {
	display: inline-block;
}

.Rcu_body {
	height: calc(100% - 40px);
	height: -webkit-calc(100% - 40px);
}
.fl {
	margin-right: 20px;
}

.rs-layout {
	padding: 10px 5px 5px 35px;
}

.rs-layout .rs-header {
	width: 95%;
	height: 50px;
	border: 1px solid #2f9279;
	box-shadow: 0px 0px 10px 1px #88f2d7 inset;
	color: #8FD8FE;
}

.rs-layout .rs-header .normal-info {
	padding: 15px 0 0 15px;
	box-sizing: border-box;
}

.rs-layout .rs-header .normal-info p:first-child {
	width: 50%;
}

.rs-layout .rs-header .normal-info p {
	width: 25%;
}

.rs-layout .rs-header .statistics-info {
	width: 48%;
	padding: 15px 15px 0 0;
	box-sizing: border-box;
	text-align: center;
}

.rs-layout .rs-header .statistics-info .rs-item {
	display: inline-block;
	margin: 0 5px;
	position: relative;
	z-index: 1;
}

.rs-layout .rs-header .statistics-info .rs-item p{
	display: inline-block;
}

.rs-layout .rs-header .statistics-info .rs-item:before {
	content: " ";
	float: left;
	width: 16px;
	height: 16px;
	margin: 1px 5px 0 0;
	background: url(/Areas/Patrol/assets/Images/blue.png) no-repeat center;
	background-size: 100%;
}
.rs-layout .rs-header .statistics-info .rs-item.skip:before {
	background-image: url(/Areas/Patrol/assets/Images/gray.png);
}
.rs-layout .rs-header .statistics-info .rs-item.failed:before {
	background-image: url(/Areas/Patrol/assets/Images/yellow.png);
}
.rs-layout .rs-header .statistics-info .rs-item.alarm:before {
	background-image: url(/Areas/Patrol/assets/Images/red.png);
}
.rs-layout .rs-header .statistics-info .rs-item.normal:before {
	background-image: url(/Areas/Patrol/assets/Images/green.png);
}

.rs-layout .rs-header .statistics-info .rs-item .item-text {
}

.Rcu_body .rs-body {

}
.Rcu_body .rs-body .rs-first-item {
	padding-left: 20px;
}
.Rcu_body .rs-body .rs-first-item .item-icon {
	width: 115px;
	height: 110px;
	background-repeat: no-repeat;
	background-position: 0 center;
	background-image: url(/Scripts/jquery-easyui-1.4.5/themes/darkgreen/images/clock.png);
	/* display: inline-block; */
	margin-right: 20px;
	font-weight: bold;
	font-size: 12px;
	color: #8FD8FE;
	float: left;
}
.Rcu_body .rs-body .rs-first-item .item-icon .item-time {
	/* margin: 45px 0 0 0px; */
	text-align: center;
}
.Rcu_body .rs-body .rs-first-item .item-icon .item-desc {
	margin: 15px 0 0 25px;
	font-size: 14px;
}

	.hide {
	display: none !important;
}

.rs-first-item .isShow {
	display: none;
}

#beginTime {
	padding-top: 50px;
	text-align: center;
}

#executeTime {
	text-align: center;
	margin-top: 50px;
}