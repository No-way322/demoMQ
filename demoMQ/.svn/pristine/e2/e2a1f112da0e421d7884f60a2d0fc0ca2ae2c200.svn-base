html, body {
	height: 100%;
}

body, ul, img, span {
	margin: 0;
	padding: 0;
}

.Rcu_body {
	position: relative;
	background: url(images/bg.png);
}

.time-list {
	width: 94%;
	margin: 0px 3% 30px;
	list-style: none;
    overflow: auto;
    padding-bottom: 80px;
}

	.time-list .first-item {
		height: 100px;
	}

	.time-list .item {
		display: block;
		padding: 10px;
	}

#head {
	height: 30px;
	margin: 10px 10px 0px 10px;
	line-height: 20px;
}

.item-time {
	width: 195px;
	height: 110px;
	float: left;
	font-size: 16px;
}

	.item-time .time-value {
		display: block;
		height: 18px;
		padding-left: 15px;
		padding-top: 21.5px;
		color: #8FD8FE;
	}

	.item-time .time-line {
		display: block;
		background-image: url('../darkgreen/images/ballline.png');
		background-repeat: no-repeat;
		height: 15px;
		background-position-x: -25px;
	}

	.item-time .time-result, .item-time .time-treeNodeName {
		display: block;
		padding-left: 15px;
		color: #8FD8FE;
	}

	.item-time .time-reason {
		display: block;
		height: 39.5px;
		padding-left: 15px;
		color: #8FD8FE;
	}

.item-overview {
	height: 100%;
	overflow: hidden;
	position: relative;
	padding: 0 10px 10px 0;
}

	.item-overview .overview-content {
		height: 50px;
		border: 1px solid #2f9279;
		box-shadow: 0px 0px 15px 2px #88f2d7 inset;
		margin: auto;
		position: absolute;
		top: 0;
		left: 0;
		bottom: 0;
		right: 0;
		padding-left: 20px;
		color: #46f3db;
	}

		.item-overview .overview-content span {
			display: block;
			height: 100%;
			line-height: 50px;
			float: left;
			margin-right: 30px;
		}

.time-list .first-item .item-icon {
	background-image: url('../darkgreen/images/clock2.png');
	background-repeat: no-repeat;
	background-position: center;
	height: 100%;
	width: 100px;
	float: left;
}

.time-list .first-item .beginTime {
	background-image: url('../darkgreen/images/clock2.png');
	background-repeat: no-repeat;
	background-position: center;
	height: 100%;
	width: 100px;
	float: left;
}

.time-list .first-item .item-icon .item-icon-title {
	font-weight: bold;
	color: #46f3db;
	position: relative;
	top: 45px;
	left: 20px;
	font-size: 13px;
}

.time-list .first-item .item-icon .item-icon-time {
	color: #46f3db;
	font-weight: bold;
	font-size: 12px;
	display: block;
	margin: auto;
	text-align: center;
	position: relative;
	top: 40px;
}

.back {
	display: block;
	background-image: url('../../../../Images/back.png');
	background-repeat: no-repeat;
	text-indent: 20px;
	background-position-y: center;
	color: #46f3db;
}

.list-item {
	background-image: url('../darkgreen/images/vertical_line.png');
	background-repeat: repeat-y;
	background-position-x: 45px;
	background-position-y: 150px;
	padding-top: 20px;
	overflow: hidden;
	display: flex;
	align-items: center;
	position: relative;
	margin-left: 50px;
}

	.list-item > div {
	}

	.list-item .item-icon {
		height: 100px;
		width: 100px;
		background-repeat: no-repeat;
		background-position: center;
		float: left;
		color: white;
		background-image: url('../../../../Images/arrived.png');
		background-size: 100px;
	}

	.list-item.arrived .item-icon {
		background-image: url('../darkgreen/images/complete.png');
	}

	.list-item.skip .item-icon {
		background-image: url('../darkgreen/images/umcomplete.png');
	}

	.list-item.skip .item-overview {
		display: none;
	}

	.list-item.no-reach .item-icon {
		background-image: url('../darkgreen/images/pass.png');
	}

	.list-item .item-icon-title {
		display: block;
		line-height: 100px;
		height: 100%;
		text-align: center;
		width: 100%;
	}

	.list-item .item-overview {
		display: flex;
		align-items: center;
	}

		.list-item .item-overview .list-table {
			/* border-radius: 0 45px; */
			padding: 10px 25px;
			width: 600px;
			overflow: hidden;
			float: left;
			vertical-align: central;
			min-height: 180px;
			border: 1px solid #2f9279;
			box-shadow: 0px 0px 10px #88f2d7 inset;
			margin: 10px;
			color: #47dae3;
		}

		/*.list-item.alarm .item-overview .list-table {
        box-shadow: 0px 0px 10px red;
    }*/

		.list-item .item-overview .list-table-head {
			margin-bottom: 10px;
			border: 1px solid #345b5b;
			/* padding-top: 10px; */
			/* height: 78px; */
			/* padding-bottom: 10px; */
		}

		.list-item .item-overview .list-table-row {
			overflow: hidden;
			color: #47dae3;
		}

		.list-item .item-overview .list-table-head .list-table-cell {
			background-repeat: no-repeat;
			background-position-y: center;
			background-size: 16px;
			text-indent: 20px;
			background-image: url('../../../../Images/baojing.png');
			color: #46f3db;
		}

			.list-item .item-overview .list-table-head .list-table-cell:first-child {
				background-image: url('../../../../Images/xjd.png');
			}

	/* .list-item .item-overview .list-table-head .list-table-cell:last-child {
    background-image: url('../../../../Images/jingdu.png');
} */
	.list-item .item-time .list-table-cell {
		background: url('../../../../Images/jingdu2.png') no-repeat 25px;
		padding-left: 45px;
		color: #60fd72;
	}

	.list-item .item-overview .list-table-cell {
		width: 25%;
		float: left;
		color: #46f3db;
		line-height: 28px;
	}

		.list-item .item-overview .list-table-cell:first-child {
			width: 60%;
		}

	.list-item .item-overview .list-table-row .list-table-cell:first-child {
		padding-top: 14px;
		width: 15%;
	}

	.list-item .item-overview .list-table-row .list-table-cell {
		height: 56px;
		width: 85%;
	}

	.list-item .item-overview .list-img {
		/* margin-top: 3px; */
		/* margin-bottom: 45px; */
		float: left;
		height: 180px;
		width: 240px;
		margin-left: 20px;
		border: 1px solid #2f9279;
		box-shadow: 0px 0px 8px #88f2d7 inset;
    padding: 8px;
    margin-top: 12px
	}

		.list-item .item-overview .list-img img {
			width: 100%;
			height: 100%;
		}

			.list-item .item-overview .list-img img[src=""], img:not([src]) {
				opacity: 0;
			}

