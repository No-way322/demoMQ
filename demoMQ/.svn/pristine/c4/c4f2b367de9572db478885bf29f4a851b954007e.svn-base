    }

#fxzd_dialogSolve a {
    display: inline-block;
    width: 65px;
    height: 19px;
   
    background-color: green;
   
    border-radius: 4px;
    color: white;
    font-size: 16px;
    text-align: center;
    line-height: 19px;
}

#fxzd_dialogSolve b {
    display: inline-block;
    width: 100px;
    height: 50px;
    position: absolute;
    background-color: green;
    bottom: 100px;
    right: 50px;
    border-radius: 4px;
    color: white;
    font-size: 16px;
    text-align: center;
    line-height: 50px;
}

