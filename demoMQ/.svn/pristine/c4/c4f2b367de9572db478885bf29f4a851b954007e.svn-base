#fxzd_dialog {
    position: relative;
}

#showHint {
    display: none;
    position: absolute;
    left: 23%;
    top: 40%;
    text-align: center;
    color: green;
    font-size: 20px;
    width: 50%;
    height: 100px;
    line-height: 100px;
}

#fxzd_dialog a {
    position: absolute;
    display: block;
    width: 80px;
    height: 80px;
    line-height: 80px;
    text-align: center;
    top: 210px;
    opacity: 0;
    border: 1px solid rgba(255,255,255,0.3);
}

    #fxzd_dialog a:nth-of-type(1) {
        background: url(../../Content/vqd/img/left.png);
    }

    #fxzd_dialog a:nth-of-type(2) {
        background: url(../../Content/vqd/img/right.png);
    }

    #fxzd_dialog a:hover {
        opacity: 1;
    }

    #fxzd_dialog a:nth-of-type(2) {
        right: 0;
    }

#fxzd_dialog img {
    width: 100%;
    height: 99%;
}

.fxzd_look,
.fxzd_slove {
    display: inline-block;
    border: 1px solid #79a294;
    width: 60px;
    height: 30px;
