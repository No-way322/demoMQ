#calendar {
    width: 90%;
    position: absolute;
    left: 5%;
    top: 50px;
}

.fc-day-header {
    background-color: #76e3d7;
    height: 46px;
    line-height: 46px;
    border: none !important;
}

.fc-head-container {
    border: none !important;
}

.workText {
    font-size: 12px;
    position: absolute;
    left: 26%;
}

#ShowBoot {
    position: absolute;
    bottom: 4px;
    left: 70px;
}

#Select {
    position: absolute;
    height: 26px;
    line-height: 26px;
    top: 0;
    right: 230px;
    letter-spacing: 2px;
}

#Speople {
    width: 120px;
    height: 28px;
    border-radius: 5px;
    outline: none;
    border-color: #ccc;
    background-color: #efefef;
}

.state {
    position: absolute;
    bottom: 4px;
    right: 70px;
    z-index: 2;
}

#Schart {
    display: none;
    position: absolute;
    top: 50px;
}

.switchbutton-off {
    background-color: #cde2f0;
    color: #56a2de;
}

.switchbutton-on {
    background-color: #a2f7c7;
    color: #2c965f;
}

#Cbutton {
    position: absolute;
    right: 3%;
    z-index: 2;
}

#Cbutton>button {
    box-sizing: border-box;
    border-radius: 4px;
    margin: 0;
    height: 2.1em;
    padding: 0 .6em;
    font-size: 1em;
    cursor: pointer;
    background-color: #f5f5f5;
    background-image: -moz-linear-gradient(top, #ffffff, #e6e6e6);
    background-image: -webkit-gradient(linear, 0 0, 0 100%, from(#ffffff), to(#e6e6e6));
    background-image: -webkit-linear-gradient(top, #ffffff, #e6e6e6);
    background-image: -o-linear-gradient(top, #ffffff, #e6e6e6);
    background-image: linear-gradient(to bottom, #ffffff, #e6e6e6);
    background-repeat: repeat-x;
    border: 1px solid;
    border-color: #e6e6e6 #e6e6e6 #bfbfbf;
    border-color: rgba(0, 0, 0, 0.1) rgba(0, 0, 0, 0.1) rgba(0, 0, 0, 0.25);
    color: #333;
    text-shadow: 0 1px 1px rgba(255, 255, 255, 0.75);
    box-shadow: inset 0 1px 0 rgba(255, 255, 255, 0.2), 0 1px 2px rgba(0, 0, 0, 0.05);
}

#Cbutton>.Disable {
    background-image: none;
    opacity: 0.65;
    box-shadow: none;
    color: #333333;
    background-color: #e6e6e6;
}

.peoplename,
.yeartotal {
    display: none;
}

i {
    font-style: normal;
}

.tabs {
    background: #40c08d;
    border-color: #40c08d !important;
}

.textbox {
    border-radius: 0px;
}