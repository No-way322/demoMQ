#sjzl_tab-tools,
#fx_tab-tools,
#xjls_tab-tools,
#ipsa_tab-tools {
    border: none;
    width: 75px;
    position: absolute;
}

    #sjzl_tab-tools a,
    #fx_tab-tools a,
    #xjls_tab-tools a,
    #ipsa_tab-tools a {
        position: absolute;
        display: block;
        width: 40px;
        height: 27px;
        line-height: 27px;
        text-align: center;
        border: 1px solid #bbb;
        background: #ffffff;
        font-weight:bold;
        color:#000;
    }

        #sjzl_tab-tools a:nth-of-type(1),
        #fx_tab-tools a:nth-of-type(1),
        #xjls_tab-tools a:nth-of-type(1),
        #ipsa_tab-tools a:nth-of-type(1) {
            border-radius: 5px 0px 0px 5px;
            border-right: none;
            text-align: left;
            padding-left: 4px;
            background: #ffe48d;
            z-index:20;
        }

            #sjzl_tab-tools a:nth-of-type(2),
            #fx_tab-tools a:nth-of-type(2),
            #xjls_tab-tools a:nth-of-type(2),
            #ipsa_tab-tools a:nth-of-type(2) {
                /*background: #ABF8CC;*/
                /*color: #2D8553;*/
                left: 30px;
                border-radius: 0px 5px 5px 0px;
                border-left:none;
                padding-right:4px;
                text-align:right;
            }

        #sjzl_tab-tools a:nth-of-type(3),
        #fx_tab-tools a:nth-of-type(3),
        #xjls_tab-tools a:nth-of-type(3),
        #ipsa_tab-tools a:nth-of-type(3) {
            background: #F4F4F4;
            border-radius: 5px;
            width: 27px;
            box-shadow: rgba(0, 0, 0, 0.3) 1px;
            z-index: 30;
        }

.tableAtcive {
    -webkit-transform: translateX(45px);
    transform: translateX(45px);
    -webkit-transition: all 0.5s;
    transition: all 0.5s;
}

.chartAtcive {
    -webkit-transform: translateX(0);
    transform: translateX(0);
    -webkit-transition: all 0.5s;
    transition: all 0.5s;
}

#sjzl_move:hover {
    box-shadow: rgba(0, 0, 0, 0.2) 2px;
}


