/*左侧树样式*/

#Org-tree {
    border-top: 1px solid #dbdbdb;
}
#tree-spliter {
    border-top:1px solid #dbdbdb;
    background-color:#f4f4f4;
}

.tree-title {
    font-size: 16px;
    font-family: 微软雅黑;
}

#search .textbox {
    border-width: 0;
}

#search .textbox-focused {
    border-width: 0;
    box-shadow: none;
}

#search .textbox-addon {
    right: 0px !important;
}


/*页面layout,grid样式*/

.panel-header,
.panel-body {
    border-color: #dbdbdb;
    font-family: 微软雅黑;
}

.datagrid-toolbar {
    padding: 0;
}

.layout-split-west {
    border-right: none;
}

.datagrid-body {
    background-color: #f4f4f4 !important;
    font-family: 微软雅黑;
    /*overflow:hidden;*/
}

.datagrid-header td,
.datagrid-body td,
.datagrid-footer td {
    border-color: #40c08d;
}
.columnGroupedTable .datagrid-header td{
 border-right: 1px solid #e2e2e2;
 border-bottom: 1px solid #e2e2e2;
}

.datagrid-header .datagrid-htable {
    height: 32px !important;
    background-color: #40c08d !important;
    border: none !important;
    color: #fff;
}

.datagrid-header .datagrid-header-inner {
    background-color: #40c08d !important;
}

.datagrid-row td {
    height: 30px;
    border-bottom: 1px solid #dbdbdb;
    border-right: 1px solid #dbdbdb;
    /*cursor: pointer;*/
}

.datagrid-row-selected {
    background-color: #BFEFFF !important;
}


/*弹出框窗体样式*/

.window {
    -webkit-border-radius: 0;
    padding: 0;
    border-radius: 0;
    border-color: #209373;
    border-width: 2px;
}

.window .window-header {
    padding: 10px 0;
}

.panel-header-noborder {
    border-width: 0;
}

.window .window-body {
    border-color: #209373;
    /*background-color: #dedede;*/
    font-size: 14px;
}

.panel-tool-close {
    background: url('/Images/closewindow.png') no-repeat !important;
    background-size: 20px !important;
}

.panel-tool a {
    width: 20px;
    height: 20px;
    opacity: 1;
}

.panel-title {
    padding-left: 15px;
    letter-spacing: 1px;
    font-size: 14px;
}

.window-content {
    border: 1px solid #bbbbbb;
    padding: 10px 15px;
    color: #000000;
    background-color: #fff;
}

.window-content .textbox {
    border-color: #bbbbbb;
    border-radius: 0;
}

.small-content {
    border: none;
    /* background-color: #dedede; */
}

.window-button {
    text-align: center;
    margin-top: 15px;
}


/*警告框样式*/

.dialog-button {
    background-color: #dedede;
}

.messager-body {
    padding: 25px 30px 35px 20px;
}


/*主要是表格样式*/

.table tr td:first-child {
    text-align: right;
}

.table tr td {
    padding: 8px 4px;
    font-size: 15px;
    font-family: 微软雅黑;
    color: #000;
}

.table input[type='text'],
.table input[type='password'] {
    width: 180px;
    font-size: 15px !important;
    font-family: 微软雅黑 !important;
}

.table textarea.textbox-text {
    font-size: 16px;
}


/*表头搜索样式*/

.grid-title {
    background-color: #f4f4f4 !important;
    padding-left: 20px;
    height: 32px;
    line-height: 32px;
    color: #188F71;
    font-weight: bold;
    letter-spacing: 1px;
    font-size: 13px;
    border-bottom: 1px solid #dbdbdb;
    position: relative;
}

.grid-search {
    background-color: #fff;
    height: 80px;
    line-height: 70px;
    font-size: 15px;
    padding-left: 15px;
    overflow-x: auto;
    overflow-y: hidden;
}

.grid-search table {
    white-space: nowrap;
}

.grid-search tr td>span:first-child {
    margin: 0 3px;
}

.grid-search .textbox {
    border-color: #dbdbdb;
    border-radius: 0;
}

.easyui-window .textbox {
    border-color: #dbdbdb;
    border-radius: 0;
}

.grid-search input[type='text'] {
    font-size: 14px;
}

.grid-line {
    width: 100%;
    height: 10px;
    border-top: 1px solid #dbdbdb;
}


