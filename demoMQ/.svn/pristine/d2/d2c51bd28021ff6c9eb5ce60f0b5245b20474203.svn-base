/*滚动条样式*/

::-webkit-scrollbar {
    width: 16px;
    height: 16px;
}

::-webkit-scrollbar-track {
    background-color: #e6e6e6;
}

::-webkit-scrollbar-thumb {
    background-color: #c4c3bc;
    /*-webkit-box-shadow:rgba(0,0,0,0.2);*/
    border-radius: 6px;
}

 ::-webkit-scrollbar-thumb:hover {
    background-color: #c4c3bc;
}

 ::-webkit-scrollbar-thumb:active {
    background-color: #bbbcbc;
}

::-webkit-scrollbar-button {
    width: 16px;
    height: 16px;
    background-repeat: repeat;
    background-size: cover;
    background-color: #e6e6e6;
}

    ::-webkit-scrollbar-button:vertical:start {
        
        background-image: url('../../images/sub_line_up.png');
    }

    ::-webkit-scrollbar-button:vertical:end {
        
        background-image: url('../../images/add_line_down.png');
    }

 ::-webkit-scrollbar-button:horizontal:start {
    background-image: url('../../images/sub_line_left.png');
}

 ::-webkit-scrollbar-button:horizontal:end {
    background-image: url('../../images/add_line_right.png');
}

::-webkit-scrollbar-corner {
    background: #f4f4f4;
}

::-webkit-scrollbar,
::-webkit-scrollbar:hover,
::-webkit-scrollbar-track {
    background-color: #2A3C4A;
}

::-webkit-scrollbar-corner,
::-webkit-scrollbar-thumb,
::-webkit-scrollbar-thumb:hover {
    background: #485D6C;
}

