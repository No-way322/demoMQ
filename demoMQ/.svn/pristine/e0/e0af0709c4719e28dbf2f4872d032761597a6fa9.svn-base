.head {
    height: 34px;
    line-height: 34px;
    background-color: #f4f4f4;
    border: 1px solid #dddddd;
    overflow: hidden;
    position: relative;
}

#headtitle {
    height: 34px;
    line-height: 34px;
    color: #40c08d;
    position: absolute;
    top: 0;
    left: 20px;
    font-weight: bolder;
    font-size: 13px;
    font-family: 微软雅黑;
}

.body {
    overflow: hidden;
    background-color: #f4f4f4;
    margin: 0;
    padding: 10px 0;
}

.content {
    height: 100%;
    overflow: hidden;
}
