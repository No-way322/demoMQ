.slide-btn {
    border: none;
    width: 120px;
    position: absolute;
    background: transparent;
    border-bottom: 1px solid #0065b3;
}

.slide-btn a {
    position: absolute;
    display: block;
    height: 27px;
    line-height: 27px;
    text-align: center;
    color: #8FD8FE;
}

.slide-btn a:nth-of-type(1) {
    width: 60px;
    background: #90d9ff !important;
    color: #1a1a1a !important;
    border-radius: 10px 0 0 10px !important;
}

.slide-btn a:nth-of-type(2) {
    width: 60px;
    background: #1794db !important;
    color: #ccfafe !important;
    left: 60px;
    border-radius: 0 10px 10px 0 !important;
}

.slide-btn a:nth-of-type(3) {
    width: 70px;
    left: -5px;
    background: #044271 !important;
    border-radius: 10px;
    box-shadow: rgba(0, 0, 0, 0.3) 1px;
}

.slide-btn .tableAtcive {
    -webkit-transform: translate(55px);
    transform: translate(55px);
    -webkit-transition: all 1s;
    transition: all 1s;
}

.slide-btn .chartAtcive {
    -webkit-transform: translate(5px);
    transform: translate(5px);
    -webkit-transition: all 1s;
    transition: all 1s;
}


