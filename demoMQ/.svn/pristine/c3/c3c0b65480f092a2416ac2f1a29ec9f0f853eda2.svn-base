.swicth {
    display: inline-block;
    width: 70%;
    max-width: 90px;
    height: 26px;
    border: 1px solid #023f44;
    border-radius: 4px;
    position: relative;
    margin-top: 4px;
    background: -webkit-linear-gradient( 90deg, rgb(1,44,49) 0%, rgb(2,75,81) 42%, rgb(8,155,165) 100%);
}

.nouse_left {
    display: block;
    position: absolute;
    top: 0;
    left: 0;
    width: 70%;
    height: 25px;
    text-align: center;
    line-height: 25px;
    cursor: pointer;
    /*background: -webkit-linear-gradient( 90deg, rgb(1,44,49) 0%, rgb(2,75,81) 42%, rgb(8,155,165) 100%);*/
    background: -webkit-linear-gradient( 90deg, rgb(8,155,165) 0%, rgb(2,75,81) 42%, rgb(1,44,49) 100%) !important;
}

.nouse_right {
    display: block;
    position: absolute;
    top: 0;
    right: 0;
    width: 40%;
    height: 26px;
    cursor: pointer;
    border-left: 1px solid #023f44;
    border-radius: 4px;
    z-index: 2;
}

.use_left {
    display: block;
    position: absolute;
    top: 0;
    left: 0;
    width: 40%;
    height: 25px;
    text-align: center;
    cursor: pointer;
    border-right: 1px solid #023f44;
    border-radius: 4px;
    z-index: 2;
    background: -webkit-linear-gradient( 90deg, rgb(8,155,165) 0%, rgb(2,75,81) 42%, rgb(1,44,49) 100%);
}

.use_right {
    display: block;
    position: absolute;
    top: 0;
    right: 0;
    width: 70%;
    height: 25px;
    background-color: #f5a450;
    line-height: 25px;
    text-align: center;
    cursor: pointer;
    background: -webkit-linear-gradient( 90deg, rgb(1,44,49) 0%, rgb(2,75,81) 42%, rgb(8,155,165) 100%);
}

.switcher {
    border: 1px solid #023f44;
    background-color: transparent;
}

.nouse_right, .switcher_btn {
    border-right: 1px solid #023f44;
    background: -webkit-linear-gradient( 90deg, rgb(1,44,49) 0%, rgb(2,75,81) 42%, rgb(8,155,165) 100%);
}

.use_left, .switcher_left_text {
    background: -webkit-linear-gradient( 90deg, rgb(8,155,165) 0%, rgb(2,75,81) 42%, rgb(1,44,49) 100%);
}

.use_right, .switcher_right_text {
    background: -webkit-linear-gradient( 90deg, rgb(1,44,49) 0%, rgb(2,75,81) 42%, rgb(8,155,165) 100%);
}

.tab {
    color: #47dae3;
    background-color: #15525a;
    /*padding: 10px 5px;*/
    padding: 5px 2px 2px;
    user-select: none;
    cursor: pointer;
}

.tab-active {
    background-color: #07353b;
}

